<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Flowers -->
  <ThingDef ParentName="PlantBase">
    <defName>SCWhitehydrangea</defName>
    <label>white hydrangea</label>
    <description>A large flowering bush with white blossoms.</description>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <Beauty>18</Beauty>
      <Nutrition>0.25</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Whitehydrangea</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <selectable>true</selectable>
    <ingestible />
    <plant>
      <fertilitySensitivity>1.0</fertilitySensitivity>
      <dieIfLeafless>true</dieIfLeafless>
      <sowWork>800</sowWork>
      <sowTags>
        <li>Ground</li>
        <li>Decorative</li>
      </sowTags>
      <growMinGlow>0.3</growMinGlow>
      <topWindExposure>0.1</topWindExposure>
      <growDays>1.5</growDays>
      <lifespanDaysPerGrowDays>5</lifespanDaysPerGrowDays>
      <visualSizeRange>0.3~1.05</visualSizeRange>
      <wildOrder>1</wildOrder>
      <purpose>Beauty</purpose>
    </plant>
  </ThingDef>
